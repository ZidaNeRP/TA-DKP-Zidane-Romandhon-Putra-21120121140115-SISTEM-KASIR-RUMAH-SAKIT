		String^ stdDetails = "{0, -10}{1, -13}{2, -13}{3, -15}{4, -15}{5, -15}";
		SpeechSynthesizer^ Sapi = gcnew SpeechSynthesizer;

	private: System::Void MyForm3_Load(System::Object^ sender, System::EventArgs^ e) {

		listBox1->Items->Add(String::Format(stdDetails, "NAMA", "NO.KTP", "DOKTER", "WAKTU"));

		cbPilihDokter->Items->Add("Dr Joko");
		cbPilihDokter->Items->Add("Drs Sekar");
		cbPilihDokter->Items->Add("Dr Heri");


	}
	private: System::Void listBox1_SelectedIndexChanged(System::Object^ sender, System::EventArgs^ e) {

	}
private: System::Void btnUpdateData_Click(System::Object^ sender, System::EventArgs^ e) {
	String^ NAMA; String^ NOKTP; String^ DOKTER; String^ WAKTU;

	NAMA = textNama->Text;
	NOKTP = textNoKTP->Text;
	DOKTER = cbPilihDokter->Text;
	WAKTU = monthCalendar1->Text;

	listBox1->Items->Add(String::Format(stdDetails, NAMA, NOKTP, DOKTER, WAKTU));

}
private: System::Void btnTambah_Click(System::Object^ sender, System::EventArgs^ e) {
	textNama->Text = "";
	textNoKTP->Text = "";
	cbPilihDokter->Text = "Pilih Dokter";
	monthCalendar1->Text = "";
}

